{
  "name": "sqlite3orm",
  "version": "2.4.6",
  "description": "ORM for sqlite3 and TypeScript/JavaScript",
  "main": "./index.js",
  "types": "./index.d.ts",
  "tags": [
    "javascript",
    "typescript",
    "sqlite",
    "ORM",
    "DAO",
    "schema",
    "database"
  ],
  "author": {
    "email": "www.gms@gmx.at",
    "name": "Guenter Sandner"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gms1/node-sqlite3-orm.git"
  },
  "bugs": {
    "url": "https://github.com/gms1/node-sqlite3-orm/issues"
  },
  "homepage": "https://github.com/gms1/node-sqlite3-orm",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p tsconfig.json",
    "rebuild": "npm run clean && npm run build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test:run": "node spec/jasmine-runner.js",
    "test": "npm run rebuild && npm run test:run",
    "watch": "tsc -w -p tsconfig.json",
    "coverage:run": "nyc -e .ts -x \"**/*.spec.*\" jasmine-ts \"./src/**/*.spec.ts\"",
    "coverage:report": "nyc report --reporter=text-lcov",
    "coverage:html": "nyc report --reporter=html",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "typedoc": "typedoc --options ./typedoc.json --out ./docs/typedoc/sqlite3orm --exclude \"**/*.spec.ts\" ./src/",
    "prepublishOnly": "echo \"ERROR: please use the dist-folder for publishing\" && exit 1",
    "release:build": "npm run clean && npm run build && npm run test:run && npm run lint && npm run _pubprep",
    "release:publish": "npm run typedoc && cd dist && npm --access public publish",
    "_pubprep": "build/sh/tsrun publish-prepare.ts"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@types/debug": "4.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jasmine": "^3.3.8",
    "@types/node": "^10.12.21",
    "@types/sqlite3": "^3.1.3",
    "coveralls": "^3.0.2",
    "fs-extra": "^7.0.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "nyc": "^13.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.1"
  },
  "typescript": {
    "definition": "./index.d.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  }
}
