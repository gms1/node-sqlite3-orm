{
  "name": "sqlite3orm",
  "version": "1.0.0",
  "description": "ORM for sqlite3 and TypeScript/JavaScript",
  "main": "./index.js",
  "types": "./index.d.ts",
  "tags": [
    "javascript",
    "typescript",
    "sqlite",
    "ORM",
    "DAO",
    "schema",
    "database"
  ],
  "author": {
    "email": "www.gms@gmx.at",
    "name": "Guenter Sandner"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gms1/node-sqlite3-orm.git"
  },
  "bugs": {
    "url": "https://github.com/gms1/node-sqlite3-orm/issues"
  },
  "homepage": "https://github.com/gms1/node-sqlite3-orm",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "rebuild": "gulp rebuild",
    "watch": "gulp watch",
    "test": "gulp test",
    "testwatch": "gulp watch --task=test",
    "coverage:run": "nyc -e .ts -x '**/*.spec.ts' jasmine-ts './src/**/*.spec.ts' --include='./src/**/*.ts' --exclude='./src/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=text-lcov",
    "coverage:html": "nyc report --reporter=html",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "typedoc": "typedoc --options typedoc.json --out docs/typedoc/sqlite3orm --exclude '**/*.spec.ts' src/",
    "prepublishOnly": "echo \"ERROR: please use the dist-folder for publishing\" && exit 1",
    "release:build": "gulp clean && gulp build test --prod",
    "release:publish": "npm run typedoc && cd dist && npm --access public publish"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.1",
    "@types/sqlite3": "^3.1.3",
    "coveralls": "^3.0.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-jasmine": "^4.0.0",
    "gulp-json-transform": "^0.4.5",
    "gulp-options": "^1.1.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "gulp-watch": "^5.0.0",
    "istanbul": "^0.4.5",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "merge-stream": "^1.0.1",
    "nyc": "^12.0.2",
    "run-sequence": "^2.2.1",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1"
  },
  "typescript": {
    "definition": "./index.d.ts"
  }
}
